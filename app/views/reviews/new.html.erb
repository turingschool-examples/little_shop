<h2>New Review for <%= @item.name %></h2>
  <%= form_tag "/items/#{@item.id}/reviews" do %>
  <%= label_tag :rating %>
  <%= text_field_tag :rating %>
<%= label_tag :title %>
<%= text_field_tag :title %>

<%= label_tag :content %>
<%= text_field_tag :content %>
<br>
  <%= submit_tag 'Post Review' %>
<% end %>
